{
  "name": "webpack-babel-express-boilerplate",
  "version": "1.0.0",
  "description": "Plain and simple boilerplate to get started with Webpack, Express and Babel",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "server": "nodemon --exec npm run babel-node -- ./src/server.js",
    "dev": "concurrently \"webpack-dev-server --mode development\" \"npm run server\"",
    "build": "concurrently \"webpack --mode production\" \"npm run server\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "express": "^4.16.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.0.7",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "postcss-loader": "^2.1.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
